<head>
    <title>Flashcards</title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="css/style.css" />
    <script src="js/jquery-1.11.3.js" type="text/javascript"></script>
    <script src="js/asignacion.js" type="text/javascript"></script>
</head>
<body>
    Grupo: <select id="grupo">
    <% for(var i=0; i < grupos.length; i++) { %>
        <option value="<%= grupos[i].id_grupo%>"><%= grupos[i].nombre %></option>
    <% } %>
    </select> <br />
    <% for(var i=0; i < alumnos.length; i++) { %>
        <input type="checkbox" class="checkbox" value="<%= alumnos[i].id_alumno%>" /><%= alumnos[i].nombre %> <br />
    <% } %>
    <input type="submit" value="Asignar" id="asignar" / >
</body>
